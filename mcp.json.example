{
  "mcpServers": {
    "gpt": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/orangekame3/gpt-mcp.git",
        "gpt-mcp"
      ],
      "env": {
        "OPENAI_API_KEY": "your-openai-api-key",
        "OPENAI_MODEL": "gpt-5",
        "OPENAI_MODEL": "gpt-5",
        "REASONING_EFFORT": "minimal",
        "VERBOSITY": "low",
        "OPENAI_API_TIMEOUT": "60",
        "OPENAI_MAX_RETRIES": "3"
      }
    }
  }
}
