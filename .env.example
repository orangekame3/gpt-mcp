# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
OPENAI_MODEL=gpt-5

# API Client Configuration
OPENAI_API_TIMEOUT=60
OPENAI_MAX_RETRIES=3

# GPT-5 Response Configuration
# Reasoning effort: minimal, low, medium, high
REASONING_EFFORT=minimal

# Verbosity level: low, medium, high
# - low: Concise, minimal output (best for coding tools)
# - medium: Balanced explanations
# - high: Detailed explanations and examples
VERBOSITY=low